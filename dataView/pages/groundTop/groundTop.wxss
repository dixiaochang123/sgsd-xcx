/* pages/groundTop/groundTop.wxss */
page {
	width: 100%;
	height: 100%;
	background-color: #e3e7f5;}
.content {
	width: 100%;
	height: 100%;
}
.banner {
	width: 100%;
	overflow: hidden;
	position: relative;
}

.banner-content{
	position: absolute;
	bottom: 70rpx;
	left: 80rpx;
	color: #ffffff;
	font-size: 35rpx;
}
.banner-content .des{
	margin-bottom: 10rpx;
	font-size: 60rpx;
	font-weight: 700;
	letter-spacing:10rpx

}
.banner .more{
	width: fit-content;
	border: 1px solid #ffffff;
	border-radius: 20rpx;
	padding: 5rpx 10rpx;
	font-size: 25rpx;
}
.banner image {
	width: 100%;
}
.warp {
	width: 92%;
	margin: 0 auto;
}
.list {
	width: 100%;
	overflow: hidden;
}
.list-li {
	width: 100%;
	height: 80rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 28rpx;
	color: #252c44;
	margin-top: 20rpx;
	border-radius: 5rpx;
}
.li-bd {
	padding-left: 20rpx;
}
.li-hd1 {
	padding-right: 30rpx;
}
.list-li:nth-child(odd) {
	background-color: #c2c9dc;
}
.list-li:nth-child(even) {
	background-color: #d4daea;
}
.list-li image {
	width: 60rpx;
	margin-right: 10rpx;
}
.li-bd {
	display: flex;
	align-items: center;
	font-size: 25rpx;
}
.li-bd text {
	display: flex;
	width: 60rpx;
	height: 60rpx;
	align-items: center;
	justify-content: center;
	font-size: 30rpx;
	color: #252c44;
	margin-right: 20rpx;
}
.swiper-tab {
	width: 100%;
	display: flex;
  	flex-direction: row;
	line-height: 60rpx;
	justify-content: space-around;
}
	
.tab-item {
  /* width: 33.3%; */
  text-align: center;
  font-size: 30rpx;
	color: #FFF;
	height: 62rpx;
	width: 175rpx;
	line-height: 62rpx;
	text-align: center;
	background-color: #9196a8;
	border-radius: 8rpx;
}
.tab-item1 {
	color: #767775;
}
.swiper {
  height: 1100px;
  background: #233440;}
	.on {
		color: #000;
		/* border-bottom: 1px solid #1BE6DA; */
		background: url(data:image/png;base64,) no-repeat center center;
		background-size: 100% 100%;
		position: relative;
	}
	.on1 {
		color: #000;
		border-bottom: solid 4rpx #e6003a;
	}
.li-hd{
	height: 100%;
	overflow: auto;
	display: flex;
  align-items: center;
}