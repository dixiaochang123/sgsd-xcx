/* pages/consumptionTop/consumptionTop.wxss */
page {
	width: 100%;
	height: 100%;
	background-color: #e3e7f5;}
.content {
	width: 100%;
}
.banner {
	width: 100%;
	overflow: hidden;
	position: relative;
}

.banner-content{
	position: absolute;
	bottom: 70rpx;
	left: 80rpx;
	color: #ffffff;
	font-size: 35rpx;
}
.banner-content .des{
	margin-bottom: 10rpx;
	font-size: 60rpx;
	font-weight: 700;
	letter-spacing:10rpx

}
.banner image {
	width: 100%;
}
.warp {
	width: 80%;
	margin: 0 auto;
}
.list {
	width: 100%;
	overflow: hidden;
}
.list-li {
	width: 100%;
	height: 80rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 28rpx;
	color: #252c44;
	margin-top: 20rpx;
}
.list-li image {
	width: 60rpx;
	margin-right: 20rpx;
}
.li-bd {
	display: flex;
	align-items: center;
}
.li-bd text {
	display: flex;
	width: 60rpx;
	height: 60rpx;
	align-items: center;
	justify-content: center;
	font-size: 30rpx;
	color: #252c44;
	margin-right: 20rpx;
}