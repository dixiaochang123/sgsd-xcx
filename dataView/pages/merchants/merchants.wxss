/* dataView/pages/merchants/merchants.wxss */
page {
	background-color: #e3e7f5;
}
.warp {
	width: 85%;
	margin: 0 auto;
}
.list {
	width: 100%;
	overflow: hidden;
}
.list-li {
	width: 100%;
	height: 80rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 28rpx;
	color: #252c44;
	margin-top: 20rpx;
	border-radius: 5rpx;
}
.list-li image {
	width: 60rpx;
	margin-right: 10rpx;
}
.li-bd {
	display: flex;
	align-items: center;
	font-size: 25rpx;
}
.li-bd text {
	display: flex;
	width: 60rpx;
	height: 60rpx;
	align-items: center;
	justify-content: center;
	font-size: 30rpx;
	color: #252c44;
	margin-right: 20rpx;
}
.li-bd {
	padding-left: 20rpx;
}
.li-hd1 {
	padding-right: 30rpx;
}
.list-li:nth-child(odd) {
	background-color: #c2c9dc;
}
.list-li:nth-child(even) {
	background-color: #d4daea;
}
.ssss {
	width: 265rpx;
	height: 100rpx;
	display: flex;
	justify-content: space-around;
	align-items: center;
	background-color: #e8ebf7;
	border-radius: 20rpx;
	box-shadow: 10rpx 10rpx 5rpx #b9b6c1;
	margin:30rpx;
}