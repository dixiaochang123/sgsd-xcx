/* dataView/pages/merchants/merchants.wxss */
.warp {
	width: 90%;
	margin: 0 auto;
}
.list {
	width: 100%;
	overflow: hidden;
}
.list-li {
	width: 100%;
	height: 80rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-size: 28rpx;
	color: #252c44;
	margin-top: 20rpx;
}
.list-li image {
	width: 60rpx;
	margin-right: 10rpx;
}
.li-bd {
	display: flex;
	align-items: center;
	font-size: 25rpx;
}
.li-bd text {
	display: flex;
	width: 60rpx;
	height: 60rpx;
	align-items: center;
	justify-content: center;
	font-size: 30rpx;
	color: #252c44;
	margin-right: 20rpx;
}