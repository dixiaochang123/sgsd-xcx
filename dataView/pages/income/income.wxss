	width: 100%;
	overflow-x: hidden;
}
.warp {
	width: 90%;
	margin: 0 auto;
}
.revenue {
	width: 100%;
	min-height: 200rpx;
	margin: 20rpx auto 50rpx;
	position: relative;
}
.title {
	width: 100%;
	display: flex;
	align-items: center;
	position: relative;
	z-index: 1;
	padding-top: 20rpx;
	padding-left: 20rpx;
}
.title image {
	width: 50rpx;
	margin-right: 20rpx;
}
.title text {
	font-size: 30rpx;
    color: #2c324d;
    padding-left: 20rpx;
    padding-top: 20rpx;
}
.revenue-list {
	width: 100%;
	min-height: 200rpx;
	/* background-color: #293c4b; */
	border-radius: 5px;
	overflow: hidden;
	margin-top: 30rpx;
	padding: 30rpx 4%;
}
.revenue-top {
	width: 100%;
