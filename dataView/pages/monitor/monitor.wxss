/* pages/monitor/monitor.wxss */
page {
	width: 100%;
	background-color: #e3e7f5;}
.content {
	width: 100%;
}
.warp {
	width: 90%;
	margin: 0 auto;
	position: relative;
	padding: 25rpx;
	margin-bottom: 20rpx;
}
.klmb {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
}
.number {
	width: 90%;
	min-height: 200rpx;
	margin: 10rpx auto 0;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-wrap: wrap;
}
.number-li {
	width: 32%;
	/* background-color: #293c4b; */
	border-radius: 5px;
	overflow: hidden;
	margin-bottom: 30rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 30rpx 8%;
	color: #040000;
	background: url(data:image/png;base64,) no-repeat center center;
	background-size: 100% 100%;
}
.number-img {
	width: 60rpx;
	display: none;
}
.number-img image {
	width: 60rpx;
}
.number-dl {
	font-size: 24rpx;
}
.number-dt {
	font-size: 25rpx;
}
.number-dd {
	margin-top: 20rpx;
	font-size: 35rpx;
}
.revenue {
	width: 100%;
	min-height: 200rpx;
	margin: 0 auto 50rpx;
}
.revenue-top {
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 50rpx;
}
.top-li {
	width: 50%;
	font-size: 28rpx;
	color: #040000;
	text-align: center;
	position: relative;
    z-index: 200;
}
.top-li text {
	padding: 16rpx 25rpx;
	background-color: #d1d6e8;
	border-radius: 9rpx;

}
.top-li.cur text {
	padding: 18rpx 27rpx;
	/* color: #1BE6DA; */
	/* border-bottom: 1px solid #1BE6DA; */
	background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100% 100%;
  position: relative;
}
.top-li.cur text::after {
	content: "";
	position: absolute;
	bottom: -8rpx;
	left: 0;
	right: 0;
	margin:auto;
	width:50%;
	height:8rpx;
	border-radius: 4rpx;
	background-color: #e5004f;

}
.title {
	width: 100%;
	display: flex;
	align-items: center;
}
.title text {
	font-size: 30rpx;
	color: #3f3d60;
}
