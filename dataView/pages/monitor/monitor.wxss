.name {
	width: 100%;
	text-align: center;
	font-size: 28rpx;
	color: #1BE6DA;
	margin-bottom: 30rpx;
}
.name text {
	border-bottom: 1px solid #1BE6DA;
}
.ec-canvas {
	width: 100%;
	height: 95%;
}
.echarts-con {
	width: 100%;
	height: 600rpx;
	position: relative;
	/* display: flex;
	align-items: center; */
}
.consumption {
	width: 100%;
	min-height: 200rpx;
	margin: 0 auto;
	position: relative;
	z-index: 1;
}
.charts-tm {
	width: 100%;
	font-size: 24rpx;
	color: #fff;
}
.tabel {
	width: 90%;
	margin: 40rpx auto;
	overflow: hidden;
	background-color: #0566E8;
}
.tabel-th {
	width: 100%;
	height: 80rpx;
	background-color: #d1d6e8;
	font-size: 28rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	text-align: center;
	color: #243441;
}
.tabel-tt {
	width: 60%;
	line-height: 80rpx;
	border-right: 1px solid #e6e9f6;}
.tabel-tts {
	width: 40%;
	line-height: 80rpx;
}
.tabel-tb {
	width: 100%;
	background-color: #fdfbfb;
	border-right: 1px solid #e6e9f6;
	border-top: 1px solid #e6e9f6;
	overflow: hidden;
}
.tabel-tr {
	width: 100%;
	height: 70rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	color: #636262;
	text-align: center;
}
.table-td {
	width: 60%;
	line-height: 70rpx;
	border-right: 1px solid #e6e9f6;
	border-top: 1px solid #e6e9f6;
}
.table-tds {
	width: 40%;
	line-height: 70rpx;
	border-top: 1px solid #e6e9f6;
}
.rightInfo {
	/* height: 100%; */
	display: flex;
	justify-content: center;
	/* flex-direction: column; */
	position: absolute;
	right: 10rpx;
	bottom:0;
	left: 0;
	right: 0;
	margin:auto;
	/* font-size: 20px; */
	color:#3f3d60;
}
.right_child {
	display: flex;
}
.rise {
	width: 23rpx;height: 30rpx;
	vertical-align: middle;
}
