/* pages/login/login.wxss */
page{
	height: 100%;
}
.login-container{
	height: 100%;
	/* background-image: url(data:image/png;base64,); */
	background-color: #e3e7f5;
	background-size: 100% 100%;
	display: flex;
	flex-direction: column;
	justify-content: center;
	padding: 55rpx;
}
.login-form{
	display: flex;
	flex-direction: column;
	width: 640rpx;
	height: 680rpx;
	background-color: #9ca0b0;
	border-radius: 20rpx;
	align-items: center;
	padding-top: 60rpx;
	text-align: center;
}
.login-form image{
	width: 300rpx;
	height: 237rpx;
	margin-bottom: 80rpx;
}
.title{
	padding: 0 0 0 40rpx;
	color: #141415;
}
.title1{
	font-size: 34rpx;
	margin-bottom: 30rpx;
}
.title2{
	font-size: 50rpx;
	margin-bottom: 90rpx;
	color: #141415;
}
.title3{
	font-size: 32rpx;
	margin-bottom: 20rpx;
	color: #FFF; 
}
.title4{
	font-size: 28rpx;
	color: #c8c8c8;
	margin-bottom: 90rpx;
}
.login-form button{
	width: 540rpx;
	height: 90rpx;
	background-color: #fffefe;
	box-shadow: -2rpx 0rpx 2rpx 2rpx #544d51;
	color:#e5004f;
	border-radius: 10rpx;
}