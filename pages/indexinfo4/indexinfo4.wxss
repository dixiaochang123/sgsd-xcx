/* pages/indexinfo4/indexinfo4.wxss */
page {
	width: 100%;
	height: 100%;
	background-color: #233440;
}
.content {
	width: 100%;
	height: 100%;
}
.warp {
	width: 92%;
	margin: 0 auto;
}
.warp-list {
	width: 100%;
	min-height: 150rpx;
	margin-bottom: 50rpx;
}
.warp-list:first-child {
	margin-top: 0;
}
.warp-ul {
	width: 100%;
	display: flex;
	align-items: center;
	flex-wrap: wrap;
}
.warp-li {
	width: 30%;
	height: 120rpx;
	font-size: 24rpx;
	color: #fff;
	text-align: center;
	margin-top: 40rpx;
	background-color: #283c4c;
	overflow: hidden;
	border-radius: 3px;
	margin-left: 5%;
}
.warp-li:first-child {
	margin-left: 0;
}
.warp-li:nth-child(3n+1) {
	margin-left: 0;
}
.warp-li image {
	width: 45rpx;
	margin-top: 10rpx;
}
.warp-li text {
	display: block;
	width: 100%;
	margin-top: 10rpx;
}